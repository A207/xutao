<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:src="@drawable/ichsagdgsak"
        android:layout_centerInParent="true"
        android:id="@+id/ceter"></ImageView>
    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:src="@drawable/ichsagdgsak"
        android:layout_toLeftOf="@+id/ceter"
        android:layout_alignBottom="@+id/ceter"></ImageView>
    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:src="@drawable/ichsagdgsak"
        android:layout_toRightOf="@+id/ceter"
        android:layout_alignBottom="@+id/ceter"></ImageView>
    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:src="@drawable/ichsagdgsak"
        android:layout_alignLeft="@+id/ceter"
        android:layout_above="@+id/ceter"></ImageView>
    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:src="@drawable/ichsagdgsak"
        android:layout_alignLeft="@+id/ceter"
        android:layout_below="@+id/ceter"></ImageView>

</RelativeLayout>